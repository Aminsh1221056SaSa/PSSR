@model PSSR.Common.CommonModels.ValueUnitModel
@{
    string sId = Model.Id.ToString();
}
<div>
    <ul class="myTreeview collaps-open">
        <li>
            <a href="#"></a>
            <a class="valueUNitNode" data-name="@Model.Name" data-id="val-@Model.Id" data-toggle="tooltip" title="@Model.ToString("URM",null)">
                @Model.Name
            </a>
            @foreach (var childComment in Model.Childeren)
            {
                @Html.Partial("TreeViewPartial/ValueUnitTreeSummary", childComment)
            }
        </li>
    </ul>
</div>